 <figure class="images-carousel-slide item <%= 'active' if active %>" id="brand-image-<%= image.imageable_id %>-<%= image.id %>">
		<% if performance %>
			<%= link_to '', images_destroy_brands_path(image.imageable_id, image.id), method: :delete, data: { confirm: 'Are you sure?' }, :remote => true, class: 'del-img' %>
		<% end %>
		<%= image_tag(image.image.remote_url, class: 'img-thumbnail custom-brand-img') %>
</figure>	

