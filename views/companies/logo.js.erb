<% if @logo.new_record? %>
alert('The photo could not be uploaded: <%= j @logo.errors.full_messages.join(', ').html_safe %>');
<% else %>
$('#company-logo #company-logo-form-container').hide();
$('#company-logo #company-logo-show-container').show();
$('#company-logo #company-logo-show-container').html('<%= j render partial: 'logo', locals: {logo: @logo} %>');
<% end %>